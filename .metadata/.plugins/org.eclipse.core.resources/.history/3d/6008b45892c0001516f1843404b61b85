package org.teachingkidsprogramming.CodingChallenge;

public class PrimeNumbers
{
  public static void main(String[] args)
  {
    for (int i = 100; i > 0; i--)
    {
      int num = (-100 + i) * -1;
      int num2 = num - 1;
      if (num % num2 != 0)
      {
        System.out.println(num);
      }
    }
  }
}
